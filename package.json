    "chai": "^3.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.10.0",
    "eslint-config-fullstack": "^3.0.0",
    "eslint-plugin-react": "^7.4.0",
    "jsdom": "11.5.1",
    "jsdom-global": "3.0.2",
    "mocha": "^3.3.0",
    "nodemon": "^1.12.1",
